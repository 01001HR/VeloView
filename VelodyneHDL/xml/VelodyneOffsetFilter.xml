<ServerManagerConfiguration>
  <!-- Begin VelodyneHDLReader -->
  <ProxyGroup name="filters">
    <SourceProxy name="VelodyneOffsetFilter" class="vtkVelodyneOffsetFilter" label="Velodyne Offset Filter">
      <Documentation
        short_help="Adjusts lidar position"
        long_help="Based on GPS.">
        Adjust lidar position from GPS
      </Documentation>

      <InputProperty command="SetInputConnection"
                     name="Input"
                     panel_visibility="default">
        <ProxyGroupDomain name="groups">
          <Group name="sources" />
          <Group name="filters" />
        </ProxyGroupDomain>
        <DataTypeDomain name="input_type">
          <DataType value="vtkPointSet" />
        </DataTypeDomain>
        <Documentation>This property specifies the input to the Transform
        filter.</Documentation>
      </InputProperty>

      <ProxyProperty command="SetInterp"
                     name="Interp"
                     panel_visibility="default">
        <Documentation>Interpolator</Documentation>
      </ProxyProperty>

      <IntVectorProperty command="SetRelativeOffset"
                         default_values="0"
                         name="RelativeOffset"
                         number_of_elements="1">
        <BooleanDomain name="bool" />
        <Documentation> Retransform back to the origin</Documentation>
      </IntVectorProperty>

    </SourceProxy>
  </ProxyGroup>
  <!-- End VelodyneHDLOffsetFilter -->
</ServerManagerConfiguration>
